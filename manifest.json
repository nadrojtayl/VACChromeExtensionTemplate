{
    "name": "Getting Started Example",
    "version": "1.0",
    "description": "Build an Extension!",
    "content_scripts": [
    {
        "js": [
                "jquery.js", 
                "contentscript.js"
            ]
    }
],
    "permissions": ["activeTab", "declarativeContent", "storage"],
    "background": {
      "scripts": ["background.js"],
      "persistent": true
    },
    "browser_action": {
      "default_popup": "popup.html"
    },
    "manifest_version": 2
  }